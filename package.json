{
  "name": "@choiceopen/automation-plugin-sdk-js",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for developing Choiceform automation plugins",
  "homepage": "https://github.com/choice-open/automation-plugin-sdk-js#readme",
  "bugs": {
    "url": "https://github.com/choice-open/automation-plugin-sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choice-open/automation-plugin-sdk-js.git"
  },
  "license": "MIT",
  "author": "Albert Yu <albert.yu@choiceform.com>",
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./schemas": {
      "development": "./src/schemas.ts",
      "default": "./dist/schemas.js"
    },
    "./types": {
      "development": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check --write",
    "dev": "tsdown --watch",
    "prepublishOnly": "bun run build",
    "test": "bun test --dots",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "type-fest": "^5.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.6",
    "@types/phoenix": "^1.6.7",
    "bumpp": "^10.4.0",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.43.0",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "chalk": "^5",
    "dotenv": "^17",
    "zod": "^4"
  },
  "peerDependenciesMeta": {
    "chalk": {
      "optional": false
    },
    "dotenv": {
      "optional": false
    },
    "zod": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./schemas": "./dist/schemas.js",
      "./types": "./dist/types.js",
      "./package.json": "./package.json"
    }
  }
}
